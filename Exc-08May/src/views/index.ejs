<!DOCTYPE html>
<html lang="vi">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Danh sách Player</title>
</head>
<body>
    <h1>Danh sách Player</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Elo</th>
            </tr>
        </thead>
        <tbody>
            <% players.forEach(player => { %>
                <tr>
                    <td><%= player.id %></td>
                    <td><%= player.name %></td>
                    <td><%= player.elo %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
